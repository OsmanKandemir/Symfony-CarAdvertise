    <section id="page-title-area" class="section-padding overlay">
        <div class="container">
            <div class="row">
                <!-- Page Title Start -->
                <div class="col-lg-12">
                    <div class="section-title  text-center">

                        <h4>{% for label, flashes in app.session.flashbag.all %}
                            {% for flash in flashes %}
                                <div style="width: 1110px;" class="alert alert-{{ label }}">
                                    {{ flash }}
                                </div></h4>
                            {% endfor %}
                        {% endfor %}

                        <h1>Profil Bilgileri</h1>



                        <span class="title-line"><i class="fa fa-car"></i></span>


                        <form method="post">
                            <h2>

                                <h1 class="h3 mb-3 font-weight-normal">Düzenle</h1> <br><br><br>
                                <h5>İsim Soyisim</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <input type="text" id="users_isimsoyisim" name="users[isimsoyisim]" value="{{ user1.isimsoyisim }}" class="form-control" placeholder="Password" required>
                                <h5>Email</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <input type="text" id="users_email" name="users[email]" value="{{ user1.email }}"  class="form-control" placeholder="Password" required>
                                <h5>Password</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <input type="text" id="users_password" name="users[password]" value="{{ user1.password }}" class="form-control" placeholder="Password" required>
                                <h5>Telefon</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <input type="text" id="users_telefon" name="users[telefon]" value="{{ user1.telefon }}" class="form-control" placeholder="Password" required>
                                <h5>Adress</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <input type="text" id="users_address" name="users[address]" value="{{ user1.address }}" class="form-control" placeholder="Password" required>
                                <h5>Şehir</h5>
                                <label for="inputPassword" class="sr-only">Password</label><br>
                                <select id="users_city"  name="users[city]" ><br>
                                    <option>{{ user1.city }}</option>
                                    <option>Adana</option>
                                    <option>Adıyaman</option>
                                    <option>Afyon</option>
                                    <option>Ağrı</option>
